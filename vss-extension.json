{
    "manifestVersion": 1,
    "id": "anchore-build-release-task",
    "name": "Anchore Build and Release Tools",
    "version": "0.0.37",
    "publisher": "FoundItSoftware",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Tools for building/releasing with Anchore Security Scanning.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Container",
        "Security",
        "Scanning"
    ],
    "icons": {
        "default": "images/anchore.png"
    },
    "branding": {
        "color": "rgb(34, 34, 34)",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://anchore.com"
        },
        "getstarted": {
            "uri": "https://docs.anchore.com/current/docs/engine/usage/integration/ci_cd/"
        },
        "learn": {
            "uri": "https://docs.anchore.com/current/docs/engine/releasenotes/"
        },
        "support": {
            "uri": "https://www.anchore.com/support"
        },
        "repository": {
            "uri": "https://github.com/anchore/anchore-engine"
        },
        "issues": {
            "uri": "https://github.com/anchore/anchore-engine/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/found-it/azure-devops-task"
    },
    "files": [
        {
            "path": "anchore-task"
        },
        {
            "path": "anchore-enterprise-task"
        },
        {
            "path": "images/anchore.png",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "engine-custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "anchore-task",
                "iconAsset": "FoundItSoftware.anchore-build-release-task/images/anchore.png",
                "includesData": {
                    "assets": [
                        "FoundItSoftware.anchore-build-release-task/images/anchore.png"
                    ]
                }
            }
        },
        {
            "id": "enterprise-custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "anchore-enterprise-task",
                "iconAsset": "FoundItSoftware.anchore-build-release-task/images/anchore.png",
                "includesData": {
                    "assets": [
                        "FoundItSoftware.anchore-build-release-task/images/anchore.png"
                    ]
                }
            }
        }
    ]
}
